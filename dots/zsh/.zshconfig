export GPG_TTY=$(tty)
export SSH_KEY_PATH="~/.ssh/rsa_id"
[[ -f ~/.aliases ]] && source ~/.aliases
[[ -f local/.aliases ]] && source ~/local/.aliases
[[ -f local/.zshrc ]] && source ~/local/.zshrc
plugins=(
  git
  bundler
  dotenv
  osx
  rake
  rbenv
  ruby
  jsontools
  node
  pip
  web-search
  colored-man-pages
  colorize
  common-aliases
  copyfile
)
source ~/dotfiles/dots/zsh/theme.zsh
